<!DOCTYPE html>
<html id="layout">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Compiled and minified CSS -->
    @Styles.Render("~/bundles/kendo.common.material")
    @Styles.Render("~/bundles/kendo.mobile")
    @Styles.Render("~/bundles/kendo.material")
    @Styles.Render("~/bundles/sitecss")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jszip")
    @Scripts.Render("~/bundles/kendojs.all")
    @Scripts.Render("~/bundles/kendojs.aspnetmvc")
    @Scripts.Render("~/bundles/kendojs.modernizr.custom")
    @Scripts.Render("~/bundles/sitejs")
</head>
<body>
<main class="body-content">
    @RenderBody()
</main>
<footer class="page-footer">
    <div class="footer-copyright">
        <p>&copy; @DateTime.Now.Year - Product Viewer Application</p>
    </div>
</footer>
@RenderSection("scripts", required: false)
</body>
</html>
